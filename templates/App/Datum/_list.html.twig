{% set values = datum.value|jsonDecode %}
<div class="input-field col col s5">
    <select id="data_{{ iteration }}_value" name="{{ entity }}[data][{{ iteration }}][value][]" data-controller="select--default" multiple>
        {% for choice in choiceList.choices %}
            <option value="{{ choice }}" {% if datum is defined and choice in values %}selected{% endif %}>{{ choice }}</option>
        {% endfor %}
    </select>
    <label for="data_{{ iteration }}_value">{{ 'label.value'|trans }}</label>
</div>
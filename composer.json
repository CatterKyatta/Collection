{
    "name": "koillection/koillection",
    "description": "Koillection is a self-hosted service allowing users to manage any kind of collection.",
    "type": "project",
    "license": "GPL-3.0-only",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "api-platform/core": "^2.6",
        "composer/semver": "^3.2",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.9",
        "doctrine/persistence": "^2.1",
        "lexik/jwt-authentication-bundle": "^2.14",
        "maennchen/zipstream-php": "^1.2",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/error-handler": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^1.6",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/security-core": "^6.0",
        "symfony/security-http": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bridge": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^6.0",
        "twig/extra-bundle": "^3.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^3.0",
        "willdurand/js-translation-bundle": "^5.0"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.10",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/maker-bundle": "^1.15",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Api\\": "api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}

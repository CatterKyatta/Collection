{
    "name": "koillection/koillection",
    "description": "Koillection is a self-hosted service allowing users to manage any kind of collection.",
    "type": "project",
    "license": "GPL-3.0-only",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "api-platform/core": "3.0.6",
        "composer/semver": "3.3.2",
        "doctrine/doctrine-bundle": "2.7.1",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.13.4",
        "doctrine/persistence": "3.1.0",
        "lexik/jwt-authentication-bundle": "2.16.0",
        "maennchen/zipstream-php": "2.4.0",
        "nelmio/cors-bundle": "2.2.0",
        "symfony/asset": "6.2.0",
        "symfony/cache": "6.2.0",
        "symfony/console": "6.2.0",
        "symfony/dependency-injection": "6.2.0",
        "symfony/dotenv": "6.2.0",
        "symfony/error-handler": "6.2.0",
        "symfony/event-dispatcher": "6.2.0",
        "symfony/expression-language": "6.2.0",
        "symfony/flex": "2.2.3",
        "symfony/form": "6.2.0",
        "symfony/framework-bundle": "6.2.0",
        "symfony/http-client": "6.2.0",
        "symfony/http-foundation": "6.2.0",
        "symfony/http-kernel": "6.2.0",
        "symfony/intl": "6.2.0",
        "symfony/mime": "6.2.0",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/process": "6.2.0",
        "symfony/property-access": "6.2.0",
        "symfony/property-info": "6.2.0",
        "symfony/proxy-manager-bridge": "6.2.0",
        "symfony/routing": "6.2.0",
        "symfony/runtime": "6.2.0",
        "symfony/security-bundle": "6.2.0",
        "symfony/security-core": "6.2.0",
        "symfony/security-http": "6.2.0",
        "symfony/serializer": "6.2.0",
        "symfony/translation": "6.2.0",
        "symfony/twig-bridge": "6.2.0",
        "symfony/twig-bundle": "6.2.0",
        "symfony/uid": "6.2.0",
        "symfony/validator": "6.2.0",
        "symfony/web-link": "6.2.0",
        "symfony/webpack-encore-bundle": "1.16.0",
        "symfony/yaml": "6.2.0",
        "twig/extra-bundle": "3.4.0",
        "twig/intl-extra": "3.4.2",
        "twig/string-extra": "3.4.0",
        "twig/twig": "3.4.3",
        "willdurand/js-translation-bundle": "dev-master"
    },
    "require-dev": {
        "brianium/paratest": "6.6.5",
        "dama/doctrine-test-bundle": "7.1.1",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "friendsofphp/php-cs-fixer": "3.13.0",
        "justinrainbow/json-schema": "5.2.12",
        "phpunit/php-code-coverage": "9.2.19",
        "phpunit/phpunit": "9.5.26",
        "rector/rector": "0.15.0",
        "symfony/browser-kit": "6.2.0",
        "symfony/css-selector": "6.2.0",
        "symfony/maker-bundle": "1.48.0",
        "symfony/phpunit-bridge": "6.2.0",
        "symfony/stopwatch": "6.2.0",
        "symfony/var-dumper": "6.2.0",
        "symfony/web-profiler-bundle": "6.2.0",
        "zenstruck/foundry": "1.24.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/benjaminjonard/BazingaJsTranslationBundle"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Api\\": "api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "Composer\\Config::disableProcessTimeout",
            "./vendor/phpunit/phpunit/phpunit --color=always --coverage-html=\"output/code-coverage\""
        ],
        "test:phpunit": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/phpunit/phpunit/phpunit"
        ],
        "test:paratest": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/paratest --runner=WrapperRunner"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.2"
        }
    }
}
